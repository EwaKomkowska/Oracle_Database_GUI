<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.put.poznan.Controllers.posilekController" prefHeight="300.0" prefWidth="500.0">
    <children>
        <Label fx:id="addLabel" alignment="CENTER" layoutX="90.0" layoutY="20.0" prefHeight="57.0" prefWidth="300.0" text="Please write new data for Posiłek" textAlignment="CENTER">
            <font>
                <Font name="Noto Sans Regular" size="15.0" />
            </font>
        </Label>
        <Pane layoutX="14.0" layoutY="68.0" prefHeight="161.0" prefWidth="330.0">
            <children>
                <TextField fx:id="nazwaField" layoutX="25.0" layoutY="100.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="200.0" text="nazwa"/>
                <TextField fx:id="godzField" layoutX="235.0" layoutY="10.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="200.0" text="godzina rozwożenia"/>
                <TextField fx:id="dietaField" layoutX="235.0" layoutY="100.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="200.0" text="dieta"/>
                <TextField fx:id="idField" layoutX="25.0" layoutY="10.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="200.0" text="idPosilku"/>
            </children>
        </Pane>
        <Button fx:id="addButton" layoutX="85.0" layoutY="250.0" mnemonicParsing="false" onMousePressed="#add" prefHeight="23.0" prefWidth="100.0" text="Add" />
        <Button fx:id="cancelButton" layoutX="295.0" layoutY="250.0" mnemonicParsing="false" onMousePressed="#cancel" prefHeight="23.0" prefWidth="100.0" text="Cancel" />
    </children>

</AnchorPane>
