<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.put.poznan.Controllers.zebranieRodziceController">
    <children>
        <Label fx:id="addLabel" alignment="CENTER" layoutX="120.0" layoutY="20.0" prefHeight="57.0" prefWidth="350.0" text="Please write new data for Zebranie z rodzicami" textAlignment="CENTER">
            <font>
                <Font name="Noto Sans Regular" size="15.0" />
            </font>
        </Label>
        <Pane layoutX="14.0" layoutY="68.0" prefHeight="161.0" prefWidth="330.0">
            <children>
                <TextField fx:id="idField" layoutX="25.0" layoutY="10.0" prefHeight="66.0" prefWidth="250.0" promptText="idZebrania" />
                <TextField fx:id="salaField" layoutX="285.0" layoutY="10.0" prefHeight="66.0" prefWidth="250.0" promptText="sala" />
                <ComboBox fx:id="obowiazkoweBox" layoutX="285.0" layoutY="100.0" prefHeight="66.0" prefWidth="250.0" promptText="czy obowiÄ…zkowe (TAK/NIE)" />
                <ComboBox fx:id="id_grupyBox" layoutX="25.0" layoutY="190.0" prefHeight="66.0" prefWidth="250.0" promptText="id grupy" />
                <ComboBox fx:id="id_przedszkolankiBox" layoutX="285.0" layoutY="190.0" prefHeight="66.0" prefWidth="250.0" promptText="id przedszkolanki" />
            <DatePicker fx:id="terminDatePicker" layoutX="24.0" layoutY="100.0" prefHeight="66.0" prefWidth="250.0" promptText="data" />
            </children>
        </Pane>
        <Button fx:id="addButton" layoutX="105.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#add" prefHeight="23.0" prefWidth="100.0" text="Add" />
        <Button fx:id="modifyButton" layoutX="105.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#update" prefHeight="23.0" prefWidth="100.0" text="Modify" />
        <Button fx:id="cancelButton" layoutX="375.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#cancel" prefHeight="23.0" prefWidth="100.0" text="Cancel" />
    </children>

</AnchorPane>
