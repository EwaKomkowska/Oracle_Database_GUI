<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.put.poznan.Controllers.PrzedszkolankaController" prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label fx:id="addLabel" alignment="CENTER" layoutX="135.0" layoutY="14.0" prefHeight="57.0" prefWidth="300.0" text="Please write new data for Przedszkolanka" textAlignment="CENTER">
            <font>
                <Font name="Noto Sans Regular" size="15.0" />
            </font>
        </Label>
        <Pane layoutX="14.0" layoutY="68.0" prefHeight="161.0" prefWidth="330.0">
            <children>
                <TextField fx:id="imieField" layoutX="8.0" layoutY="7.0" prefHeight="66.0" prefWidth="250.0" promptText="imie"/>
                <TextField fx:id="nazwiskoField" layoutX="8.0" layoutY="87.0" prefHeight="66.0" prefWidth="250.0" promptText="nazwisko"/>
                <TextField fx:id="kwalifikacjeField" layoutX="8.0" layoutY="167.0" prefHeight="66.0" prefWidth="250.0" promptText="kwalifikacje"/>
                <TextField fx:id="idField" layoutX="8.0" layoutY="247.0" prefHeight="66.0" prefWidth="250.0" text="id">
                    <font>
                        <Font name="Noto Sans Regular" size="14.0" />
                    </font>
                </TextField>
                <TextField fx:id="placaField" layoutX="278.0" layoutY="7.0" prefHeight="66.0" prefWidth="250.0" promptText="placa"/>
                <ComboBox fx:id="id_grupyBox" layoutX="278.0" layoutY="87.0" prefHeight="66.0" prefWidth="250.0" promptText="id grupy"/>
                <ComboBox fx:id="id_hospitacjiBox" layoutX="278.0" layoutY="167.0" prefHeight="66.0" prefWidth="250.0" promptText="id hospitacji"/>
            </children>
        </Pane>
        <Button fx:id="addButton" layoutX="312.0" layoutY="320.0" mnemonicParsing="false" onMousePressed="#add" prefHeight="23.0" prefWidth="100.0" text="Add" />
        <Button fx:id="modifyButton" layoutX="312.0" layoutY="320.0" mnemonicParsing="false" onMousePressed="#update" prefHeight="23.0" prefWidth="100.0" text="Modify" />
        <Button fx:id="cancelButton" layoutX="432.0" layoutY="320.0" mnemonicParsing="false" onMousePressed="#cancel" prefHeight="23.0" prefWidth="106.0" text="Cancel" />
    </children>

</AnchorPane>

