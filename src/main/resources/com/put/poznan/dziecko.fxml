<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.put.poznan.Controllers.dzieckoController">
    <children>
        <Label fx:id="addLabel" alignment="CENTER" layoutX="150.0" layoutY="20.0" prefHeight="57.0" prefWidth="300.0" text="Please write new data for Dziecko" textAlignment="CENTER">
            <font>
                <Font name="Noto Sans Regular" size="15.0" />
            </font>
        </Label>
        <Pane layoutX="14.0" layoutY="68.0" prefHeight="161.0" prefWidth="330.0">
            <children>
                <TextField fx:id="idField" layoutX="25.0" layoutY="10.0" prefHeight="66.0" prefWidth="250.0" promptText="idDziecka" />
                <TextField fx:id="imieField" layoutX="25.0" layoutY="100.0" prefHeight="66.0" prefWidth="250.0" promptText="imie" />
                <TextField fx:id="nazwiskoField" layoutX="285.0" layoutY="10.0" prefHeight="66.0" prefWidth="250.0" promptText="nazwisko" />
                <ComboBox fx:id="id_grupyBox" layoutX="25.0" layoutY="190.0" prefHeight="66.0" prefWidth="250.0" promptText="id grupy" />
                <ComboBox fx:id="id_posilkuBox" layoutX="285.0" layoutY="190.0" prefHeight="66.0" prefWidth="250.0" promptText="id posilku" />
            </children>
        </Pane>
        <Button fx:id="addButton" layoutX="105.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#add" prefHeight="23.0" prefWidth="100.0" text="Add" />
        <Button fx:id="modifyButton" layoutX="105.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#update" prefHeight="23.0" prefWidth="100.0" text="Modify" />
        <Button fx:id="cancelButton" layoutX="375.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#cancel" prefHeight="23.0" prefWidth="100.0" text="Cancel" />
      <DatePicker fx:id="dataDatePicker" layoutX="300.0" layoutY="169.0" prefHeight="66.0" prefWidth="250.0" promptText="data urodzenia" style="-fx-background-color: lightgrey;" />
    </children>

</AnchorPane>
