<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.put.poznan.Controllers.zajeciaDodatkoweController">
    <children>
        <Label fx:id="addLabel" alignment="CENTER" layoutX="150.0" layoutY="20.0" prefHeight="57.0" prefWidth="300.0" text="Please write new data for Zajecia dodatkowe" textAlignment="CENTER">
            <font>
                <Font name="Noto Sans Regular" size="15.0" />
            </font>
        </Label>
        <Pane layoutX="14.0" layoutY="68.0" prefHeight="161.0" prefWidth="330.0">
            <children>
                <TextField fx:id="idField" layoutX="25.0" layoutY="10.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="250.0" text="idZajecia" />
                <TextField fx:id="rodzajField" layoutX="25.0" layoutY="100.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="250.0" text="rodzaj" />
                <TextField fx:id="czasField" layoutX="285.0" layoutY="100.0" onMouseClicked="#clear" prefHeight="66.0" prefWidth="250.0" text="czas tygodniowo (w godz)" />
                <ComboBox fx:id="id_grupyBox" layoutX="25.0" layoutY="190.0" prefHeight="66.0" prefWidth="250.0" promptText="id grupy" />
                <ComboBox fx:id="id_oplatyBox" layoutX="285.0" layoutY="190.0" prefHeight="66.0" prefWidth="250.0" promptText="id oplaty" />
            </children>
        </Pane>
        <Button fx:id="addButton" layoutX="105.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#add" prefHeight="23.0" prefWidth="100.0" text="Add" />
        <Button fx:id="cancelButton" layoutX="375.0" layoutY="350.0" mnemonicParsing="false" onMousePressed="#cancel" prefHeight="23.0" prefWidth="100.0" text="Cancel" />
      <DatePicker fx:id="terminDatePicker" layoutX="300.0" layoutY="77.0" prefHeight="66.0" prefWidth="250.0" promptText="data prowadzenia" />
    </children>

</AnchorPane>
